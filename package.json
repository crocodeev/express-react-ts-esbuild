{
  "name": "express-react-ts-esbuild",
  "version": "1.0.0",
  "description": "boilerplate for fast start develop with express + react + typescript",
  "main": "index.js",
  "scripts": {
    "dev:client": "node ./esbuild/client.dev-config.js",
    "dev:server": "node ./esbuild/server.dev-config.js",
    "dev:server-watch": "nodemon ./build/index.js",
    "develop": "npm-run-all --parallel dev:*"
  },
  "keywords": [
    "express",
    "typescript",
    "react",
    "esbuild"
  ],
  "author": "crocodeev",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "chokidar": "^3.5.3",
    "esbuild": "^0.12.15",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "servor": "^4.0.2",
    "typescript": "^5.1.6"
  }
}
